/* You can add global styles to this file, and also import other style files */
@import '~@angular/material/prebuilt-themes/indigo-pink.css'

html
body
	font-size 14px


:root
	--padding: .5rem
	--margin: 1rem

ul
	list-style: none
	margin: 0
	padding: 0
	display flex

a
a:hover,
a:active,
a:focus,
a:visited
	text-decoration none
	outline: none


*,
*:before,
*:after
    box-sizing: inherit
	box-sizing: border-box;



img
    border-style: none

